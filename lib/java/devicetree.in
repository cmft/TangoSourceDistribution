#!@SHELL@

if [ ! $TANGO_HOST ] && [ -f @TANGO_RC_FILE@ ]; then
   . @TANGO_RC_FILE@
fi

JAVALIB=@prefix@/share/java;
JAVABIN=@prefix@/share/java;
CLASSPATH=$JAVALIB/TangORB.jar:$JAVALIB/ATKWidget.jar:$JAVALIB/ATKCore.jar:$JAVABIN/Jive.jar:$JAVABIN/atkpanel.jar:$JAVABIN/DeviceTree.jar
export CLASSPATH

LIBPATH=@libdir@
export LIBPATH

echo  Running Device Tree...
@JAVA@ -Djava.library.path=$LIBPATH -DTANGO_HOST=$TANGO_HOST explorer.Main $@
