#@SHELL@

if [ ! $TANGO_HOST ] && [ -f @TANGO_RC_FILE@ ]; then
   . @TANGO_RC_FILE@
fi

JAVALIB=@prefix@/share/java;

TANGOATK=$JAVALIB/ATKCore.jar:$JAVALIB/ATKWidget.jar

CLASSPATH=$JAVALIB/TangORB.jar:$JAVALIB/Jive.jar:$TANGOATK
export CLASSPATH

LIBPATH=@prefix@/lib
export LIBPATH

echo   Starting...

@JAVA@  -Djava.library.path=$LIBPATH  -DTANGO_HOST=$TANGO_HOST jive.ExecDev $1
